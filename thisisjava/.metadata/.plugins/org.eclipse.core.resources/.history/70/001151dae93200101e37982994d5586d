package checkTest;

public class Account {
	
	private int accountN;
	private String name;
	private int money;
	
	public Account(int accountN, String name, int money){
		this.accountN = accountN;
		this.name = name;
		this.money = money;
	}
	
	public int getAccountN() {
		return accountN;
	}
	
	public String getName() {
		return name;
	}
	
	public int getMoney() {
		return money;
	}
	
	public void setAccountN(int accountN) {
		this.accountN = accountN;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setMoney(int money) {
		this.money = money;
	}

}
